{
  "env": {
    "commonjs": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ],
      "presets": [
        "es2015",
        "react",
        "stage-1"
      ]
    },
    "es": {
      "plugins": [
        "transform-runtime",
        "transform-object-assign"
      ],
      "presets": [
        "es2015-rollup",
        "react",
        "stage-1"
      ]
    },
    "amd": {
      "plugins": [
        "transform-runtime",
        "transform-object-assign",
        "transform-es2015-modules-amd"
      ],
      "presets": [
        "es2015-rollup",
        "react",
        "stage-1"
      ]
    },
    "production": {
      "comments": false,
      "compact": true,
      "plugins": [
        "transform-runtime",
        "transform-object-assign"
      ],
      "presets": [
        "es2015",
        "react",
        "stage-1"
      ]
    }
  }
}
